{
    "name": "Team Rater",
    "description" : "Rates PS! teams",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },

    "content_scripts": [{
        "js": ["code.js"],
        "matches": ["https://play.pokemonshowdown.com/teambuilder"]
    }],

    "permissions": ["tabs", "<all_urls>"]
  }